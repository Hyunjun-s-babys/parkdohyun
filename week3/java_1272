import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
       
        int k = sc.nextInt();
        int h = sc.nextInt();

        int kn = k%2==0? k*5 : (k+1)/2;
        int hn = h%2==0? h*5 : (h+1)/2; 

        System.out.println(kn+hn);

        }
    } 

