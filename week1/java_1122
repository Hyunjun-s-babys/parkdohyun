import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        
        Scanner sc = new Scanner(System.in);

        int sec = sc.nextInt();
        int hour = 0 ;
        while (sec >= 60) {
            sec = sec - 60;
            hour = hour + 1;

        }

       System.out.printf("%d %d", hour, sec ) ;

    }
}
