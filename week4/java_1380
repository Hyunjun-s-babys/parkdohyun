import java.util.Scanner;

public class Main{
    public static void main(String[] args){
       
        Scanner sc = new Scanner(System.in);

        int k = sc.nextInt();

        for(int i=1;i<=6;i++){
            if(k-i<7&&k-i>0){
                System.out.printf("%d %d\n",i,k-i);
            }
        }
        
    }
}
