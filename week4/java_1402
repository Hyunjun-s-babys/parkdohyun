import java.util.Scanner;
import java.util.Arrays;
public class Main{
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
       int N = sc.nextInt();
       int[] arr = new int[N];

       for(int i=0;i<N;i++){
        arr[N-i-1] = sc.nextInt();
       }
       for(int A:arr){
        System.out.print(A+" ");
       }

    }
}


