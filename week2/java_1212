import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
       
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();

        int x = (a+b>c&&a+c>b&&b+c>a?1:0);

        if(x==1){
            System.out.println("yes");
        }
        else{
            System.out.println("no");
        }

        
    } 
}
