import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
       
        double a = sc.nextDouble();
        double b = sc.nextDouble();
        double c = sc.nextDouble();
        double d = sc.nextDouble();
        if(a/b>c/d){
            System.out.println(">");
        }
        else if(a/b==c/d){
            System.out.println("=");
        }
        else{
            System.out.println("<");
        }
    } 
}



