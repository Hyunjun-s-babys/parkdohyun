import java.util.Scanner;

public class Main {
    public static void main(String[] args){
        
        Scanner sc = new Scanner(System.in);

        int a = sc.nextInt();
        int b = sc.nextInt();
        
        boolean result = a==b; 

        if(result == true){
            System.out.println(1);
        }
        if(result == false){

            System.out.println(0);
        }

       

    }
}
